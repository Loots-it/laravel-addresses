{
    "name": "loots-it/laravel-address-creator",
    "description": "This package provides an Address Model, Migration and Factory. It also provides the traits HasBillingAddres and HasPublicAddress",
    "keywords": [
        "address",
        "laravel",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Laurens Loots",
            "email": "laurens@loots-it.be"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/database": "^8.14",
        "php": "^8.0",
        "rinvex/countries": "^7.3",
        "watson/validating": "^7.0",
        "illuminate/console": "^8.14",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "laravel/framework": "^8.14",
        "loots-it/address-model": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LootsIt\\Address\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LootsIt\\Address\\Providers\\AddressServiceProvider"
            ]
        }
    }
}
